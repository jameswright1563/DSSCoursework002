<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body data-home-page="login.html" data-home-page-title="Home" class="u-body u-xl-mode"><div class="u-clearfix u-sheet u-valign-middle u-sheet-1">
        <%- include('../partials/navbar', {loggedin:loggedin}); %>
        <section>
            <h1 align="center">Food Blog</h1>
        </section>
        <div>
            <% test.forEach(function(test) { %>
                <div class="posts">
                    <h3 align="center"><%= test.title%></h3>
                    <img src="data:image/<%=test.image.contentType%>;base64,
                     <%=test.image.data.toString('base64')%>" align="center" onclick="testFunction()">
                    <p><%= test.description%></p>
                    <hr class="rounded1">
                    <i onclick="likedislike(this)" class="fa fa-thumbs-up"></i>
                    <i onclick="comments(this)"></i>
                </div>
                <br>
            <% }); %>
        </div>
</div>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>
<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body data-home-page="login.html" data-home-page-title="Home" class="u-body u-xl-mode"><div class="u-clearfix u-sheet u-valign-middle u-sheet-1">
        <%- include('../partials/navbar', {loggedin:loggedin}); %>
        <section>
            <h1 align="center">Food Blog</h1>
        </section>
        <div>
            <% for(var i=0; i<test.length; i++) { %>
                <div class="posts">
                    <h3 align="center"><%= test[i].title%></h3>
                    <%if(locals.test[i].image.contentType){%>
                    <img src="data:image/<%=test[i].image.contentType%>;base64,
                     <%=test[i].image.data.toString('base64')%>" align="center" onclick="testFunction()">
                    <%}else{%>
                    <%console.log("yes")}%>
                    <p><%= test[i].description%></p>
                    <hr class="rounded1">
                    <i onclick="likedislike(this)" class="fa fa-thumbs-up"></i>
                    <i onclick="comments(this)"></i>
                </div>
                <br>
            <% } %>
        </div>
</div>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>